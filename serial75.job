set echo

ja

#move to my $SCRATCH directory
cd $SCRATCH

#copy executable to $SCRATCH
cp $HOME/kenken/serial serial75
cp $HOME/kenken/input/75minutes.txt	75minutes.txt


#run my executable
time ./serial75 75minutes.txt

ja -chlst
